<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>navController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.NavController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>userSession</property-name>
            <value>#{userSession}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>userSession</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.login.Session</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>headerController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.home.HeaderController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>userSession</property-name>
            <value>#{userSession}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>signupController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.signup.SignupController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>createEmplController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.admin.CreateEmployeeController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>adminHomeController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.admin.AdminHomeController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>editMenuItemController</property-name>
            <value>#{editMenuItemController}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>editMenuItemController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.admin.EditMenuItemController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>addMenuItemController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.admin.AddMenuItemController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>editCustomizationController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.admin.EditCustomizationController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>emplOrderController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.employee.OrderListController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>shoppingCart</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.cart.ShoppingCartController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>addItemController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.cart.AddItemController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>shoppingCart</property-name>
            <value>#{shoppingCart}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>menuController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.menu.MenuController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>userSession</property-name>
            <value>#{userSession}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>cardController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.customer.CreditCardController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>userSession</property-name>
            <value>#{userSession}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>addCardController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.customer.AddCreditCardController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>userSession</property-name>
            <value>#{userSession}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>checkoutController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.cart.CheckoutController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>shoppingCart</property-name>
            <value>#{shoppingCart}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>orderDetailsController</managed-bean-name>
        <managed-bean-class>com.stanchefskitchen.presentation.employee.OrderDetailsController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
